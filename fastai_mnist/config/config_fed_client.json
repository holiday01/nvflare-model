{
  "format_version": 2,

  "executors": [
    {
      "tasks": [
        "train", "submit_model", "validate"
      ],
      "executor": {
        "id": "Executor",
        "path": "nvflare.app_common.executors.learner_executor.LearnerExecutor",
        "args": {
          "learner_id": "cifar10-learner"
        }
      }
    }
  ],

  "task_result_filters": [
  ],
  "task_data_filters": [
  ],

  "components": [
    {
      "id": "learner",
      "path": "learners.learner.Learner",
      "args": {
        "aggregation_epochs": 1,
        "lr": 1e-4
      }
    }
  ]
}
